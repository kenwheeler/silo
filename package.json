{
  "name": "@thekenwheeler/silo",
  "version": "0.0.1",
  "description": "RxJs based data management",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "flow": "flow start",
    "lint": "eslint src test",
    "test": "mocha --compilers js:babel-register --recursive --harmony-proxies",
    "test:watch": "npm test -- --watch",
    "test:cov": "babel-node node_modules/isparta/bin/isparta cover --config .isparta.server.yml node_modules/mocha/bin/_mocha",
    "check-ci": "npm run lint && npm run test:cov"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kenwheeler/silo.git"
  },
  "keywords": [
    "rx",
    "rxjs",
    "react",
    "data"
  ],
  "author": "Ken Wheeler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kenwheeler/silo/issues"
  },
  "homepage": "https://github.com/kenwheeler/silo#readme",
  "dependencies": {
    "rxjs": "^5.0.0-beta.2"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "coveralls": "^2.11.8",
    "cross-env": "^1.0.7",
    "eslint": "^1.8.0",
    "eslint-config-defaults": "^7.1.1",
    "eslint-plugin-filenames": "^0.1.2",
    "eslint-plugin-react": "^3.6.3",
    "expect": "^1.8.0",
    "isparta": "^4.0.0",
    "mocha": "^2.2.5",
    "rimraf": "^2.4.4",
    "webpack": "^1.12.8"
  }
}
